comp_4=`
comp_5=`ret = (ret.y-ret.x*0.5)*lerp(float3(1.8,2.3,1.1),1,ret.x)*1.4;
comp_6=`ret *= 1.99; //MilkDrop2077 gamma effect
comp_7=`
comp_8=`
comp_9=`ret *= float3(rad*3,1,1);
comp_10=`}
